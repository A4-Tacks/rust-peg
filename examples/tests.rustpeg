#[export]
consonants
	= ([a-z]![aeiou])+

#[export]
options -> Option<()>
	= "abc" v:"def"? {v}

number -> int
	= [0-9]+ { from_str::<int>(match_str).unwrap() }

#[export]
list -> Vec<int>
	= number ** ","

#[export]
boundaries -> String
	= "foo" { match_str.to_string() }

#[export]
borrowed -> &str
	= [a-z]+ { match_str }

#[export]
block -> &str
	= [a-z]+ {
		let result = match_str;
		result
	}
